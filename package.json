{
  "name": "googledoc-extract-images",
  "version": "0.2.0",
  "description": "Извлекает все изображения из гугл-дока и нумерует их в порядке очередности",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-lin": "electron-packager . --overwrite --out=build --icon=icon.ico --platform=linux",
    "build-win": "electron-packager . --overwrite --out=build --icon=icon.ico --platform=win32"
  },
  "repository": "https://github.com/Morion-Self/googledoc-extract-images",
  "author": "Marat Talipov",
  "devDependencies": {
    "electron": "^12.0.0",
    "electron-packager": "^15.2.0"
  },
  "dependencies": {
    "fs": "^0.0.1-security",
    "node-html-parser": "^3.0.4",
    "os": "^0.1.1",
    "request": "^2.88.2",
    "unzipper": "^0.10.11"
  }
}
